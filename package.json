{
  "name": "axolotl-osint",
  "version": "1.0.0",
  "description": "Scrape websites for the purpose of OSINT research",
  "bin": {
    "cli-typescript-starter": "./bin/run"
  },
  "directories": {
    "lib": "src",
    "bin": "bin"
  },
  "files": [
    "dist",
    "bin"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/axolotl-logic/axolotl-osint.git"
  },
  "scripts": {
    "build": "tsup-node",
    "build:watch": "tsup-node --watch",
    "clean": "rimraf dist",
    "commit": "cz",
    "commitlint": "commitlint --edit",
    "compile": "tsc",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "ts-node ./bin/run.ts",
    "start:node": "node ./bin/run",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "prepare": "husky",
    "release": "semantic-release"
  },
  "keywords": [
    "typescript",
    "starter",
    "cli",
    "bootstrap"
  ],
  "author": "Andrey Kucherenko <kucherenko.andrey@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@jest/globals": "^30.0.3",
    "@tsconfig/node20": "^20.1.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.6",
    "@types/prompts": "^2.4.9",
    "@types/signale": "^1.4.7",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "jest": "^30.0.3",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.5",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "consola": "^3.4.2",
    "dotenv": "^16.6.1",
    "giget": "^2.0.0",
    "picocolors": "^1.1.1",
    "typescript-eslint": "^8.35.0",
    "yargs": "^18.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
